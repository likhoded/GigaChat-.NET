<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Description>About
Unofficial .NET Library for working with Giga Chat AI by Sber/ Неофициальная .NET библиотека для работы с GigaChat AI от Сбера</Description>
    <PackageProjectUrl>https://github.com/likhoded/GigaChatSDK</PackageProjectUrl>
    <RepositoryUrl>https://github.com/likhoded/GigaChatSDK</RepositoryUrl>
    <PackageTags>GigaChat</PackageTags>
    <Version>1.0.5</Version>
    <Title>GigaChatSDK</Title>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http.Json" Version="8.0.0" />
  </ItemGroup>

</Project>
